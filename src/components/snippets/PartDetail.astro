---
import Icon from "@/components/Icon";

interface Props {
    part: any;
}
const { part, ...attrs } = Astro.props;
---

<div class="flex flex-col gap-2">
    <Icon icon="hero/CheckCircleFill" class="text-black w-5 h-5" />

    <div class="font-semibold">{part.title}</div>
    <div class="text-xs line-clamp-4">{part.description}</div>
</div>
